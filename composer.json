{
    "name": "elcweb/salesforce-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily access salesforce objects",
    "license": "MIT",
    "authors": [
        {
            "name": "Etienne Lachance",
            "email": "el@elcweb.ca"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle"  : ">=2.1,<3.0-dev",
        "phpforce/common"           : "dev-master",
        "phpforce/soap-client"      : "0.1.0",
        "leaseweb/memcache-bundle"  : "*"
    },
    "suggest": {
        "elcweb/keyvaluestore-bundle": "Needed to manage configuration via the database"
    },
    "autoload": {
        "psr-0": { "Elcweb\\SalesforceBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Elcweb/SalesforceBundle"
}
